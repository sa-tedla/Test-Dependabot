{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 5175",
    "dev:docker": "node --inspect=0.0.0.0:9229 node_modules/.bin/vite dev --config vite.docker.config.ts",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "fmt": "prettier --write --plugin prettier-plugin-svelte .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.0",
    "@melt-ui/svelte": "^0.76.3",
    "@playwright/test": "^1.37.1",
    "@rgossiaux/svelte-headlessui": "^2.0.0",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "autoprefixer": "^10.4.15",
    "bits-ui": "^0.22.0",
    "clsx": "^2.1.1",
    "daisyui": "^3.6.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.37.0",
    "lint-staged": "^15.5.2",
    "lucide-svelte": "^0.469.0",
    "path": "^0.12.7",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.2.3",
    "prisma": "^5.2.0",
    "svelte": "^4.2.14",
    "svelte-check": "^3.6.9",
    "sveltekit-superforms": "^2.12.6",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^1.0.0",
    "zod": "^3.22.2"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "@sentry/sveltekit": "^10.5.0",
    "@svelte-put/clickoutside": "^3.0.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "ai": "^2.2.9",
    "crypto-js": "^4.1.1",
    "emoji-picker-element": "^1.21.1",
    "gpt3-tokenizer": "^1.1.5",
    "mssql": "^12.2.0",
    "nanoid": "^4.0.2",
    "openai": "^4.2.0",
    "php-serialize": "^4.1.1",
    "svelte-autosize": "^1.1.0",
    "svelte-material-icons": "^3.0.5",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "overrides": {
    "@rgossiaux/svelte-headlessui": {
      "svelte": "$svelte"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.svelte": [
      "prettier --write --plugin prettier-plugin-svelte",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
