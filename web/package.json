{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 5175",
    "dev:docker": "node --inspect=0.0.0.0:9229 node_modules/.bin/vite dev --config vite.docker.config.ts",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "fmt": "prettier --write --plugin prettier-plugin-svelte .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.86.6",
    "@playwright/test": "^1.57.0",
    "@rgossiaux/svelte-headlessui": "^2.0.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "autoprefixer": "^10.4.22",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "daisyui": "^5.5.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "lint-staged": "^16.2.7",
    "lucide-svelte": "^0.555.0",
    "path": "^0.12.7",
    "postcss": "^8.4.28",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "prisma": "^7.1.0",
    "svelte": "^5.45.5",
    "svelte-check": "^4.3.4",
    "sveltekit-superforms": "^2.28.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^4.0.15",
    "zod": "^3.22.2"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^7.1.0",
    "@sentry/sveltekit": "^10.29.0",
    "@svelte-put/clickoutside": "^4.0.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "ai": "^5.0.106",
    "crypto-js": "^4.1.1",
    "emoji-picker-element": "^1.28.0",
    "gpt3-tokenizer": "^1.1.5",
    "mssql": "^12.2.0",
    "nanoid": "^5.1.6",
    "openai": "^6.10.0",
    "php-serialize": "^5.1.3",
    "svelte-autosize": "^1.1.5",
    "svelte-material-icons": "^3.0.5",
    "ts-node": "^10.9.1",
    "uuid": "^13.0.0"
  },
  "overrides": {
    "@rgossiaux/svelte-headlessui": {
      "svelte": "$svelte"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.svelte": [
      "prettier --write --plugin prettier-plugin-svelte",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
